            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".personal_center.AboutActivity"
            android:label="@string/app_name_about"
            android:theme="@style/AppTheme_Account" />
        <activity
            android:name=".personal_center.AccountActivity"
            android:label="@string/app_name_account"
            android:theme="@style/AppTheme_Account" />
        <activity android:name=".personal_center.RecorderHistoryActivity"
            android:label="@string/app_name_recorderHistory"
            android:theme="@style/AppTheme_Account"/>
        <activity android:name=".personal_center.RecorderNowActivity"
            android:label="@string/app_name_recorderNow"
            android:theme="@style/AppTheme_Account"/>
